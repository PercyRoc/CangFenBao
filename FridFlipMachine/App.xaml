<dryIoc:PrismApplication x:Class="FridFlipMachine.App"
                         xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                         xmlns:dryIoc="http://prismlibrary.com/"
                         xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml" x:ClassModifier="internal">
    <dryIoc:PrismApplication.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ui:ThemesDictionary Theme="Light" />
                <ui:ControlsDictionary />
                <ResourceDictionary Source="pack://application:,,,/HandyControl;component/Themes/SkinDefault.xaml" />
                <ResourceDictionary Source="pack://application:,,,/HandyControl;component/Themes/Theme.xaml" />
                <ResourceDictionary
                    Source="pack://application:,,,/SharedUI;component/Resources/Styles/ColorStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </dryIoc:PrismApplication.Resources>
</dryIoc:PrismApplication>